    "rc-resize-observer": "^1.4.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-on-screen": "^2.1.1",
    "react-scroll-into-view-if-needed": "^3.0.1",
    "react-select": "^5.7.7",
    "react-table": "^7.8.0",
    "react-table-6": "^6.11.0",
