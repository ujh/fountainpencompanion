    "jquery-ui": "^1.14.0",
    "jquery-ujs": "^1.2.3",
    "js-cookie": "^3.0.5",
    "jsona": "^1.12.1",
    "jstz": "^2.1.1",
    "lint-staged": "^15.2.9",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.4",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss-preset-env": "^10.0.2",
