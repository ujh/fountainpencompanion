    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "react-on-screen": "^2.1.1",
    "react-scroll-into-view-if-needed": "^3.0.1",
    "react-select": "^5.8.0",
    "react-table": "^7.8.0",
    "react-table-6": "^6.11.0",
    "recharts": "2.12.5",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "select2": "4.0.13",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-remove-empty-scripts": "^1.0.4",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@prettier/plugin-ruby": "^4.0.4",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "1.0.1",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": "20.3.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/javascript/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "text-summary"
    ],
    "modulePaths": [
      "<rootDir>/app/javascript/src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/spec/javascript/support/identity-mapper.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/spec/javascript/support/setup.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(d3-scale-chromatic|d3-interpolate|d3-color)/)"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "lint": "yarn run prettier && yarn run eslint",
    "eslint": "eslint . --ext .js --ext .jsx",
    "prettier": "prettier --check .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{css,html,js,json,jsx,md,rb,ru,scss,yml,yaml,rake}": "prettier --write",
    "*.{js,jsx}": "eslint --fix"
  }
}
